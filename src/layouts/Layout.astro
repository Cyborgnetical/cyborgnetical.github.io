---
interface Props {
	title: string;
}

const { title } = Astro.props;
import Header from '../components/Header.astro';
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content=" The portfolio of Cyborgnetical." />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.ico" />
		<meta name="generator" content={Astro.generator} />
		<title>Cyborgnetical - {title}</title>
	</head>
	<body>
        <Header></Header>
		<slot />
	</body>
</html>
<style is:global>

</style>
